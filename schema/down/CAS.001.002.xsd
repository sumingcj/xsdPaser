<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="Document">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MainBody"/>
				<xs:element name="CdesSgntr" type="Signature"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MainBody">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MsgId"/>
				<xs:element ref="StlInf"/>
				<xs:element ref="StlRstInf"/>
				<xs:element ref="ExtInfs" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MsgId">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Id" type="Max35NumericText"/>
				<xs:element name="CreDtTm" type="ISODateTime"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="StlInf">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="DlvOrderNo" type="Max24AlphaNumericText"/>
				<xs:element name="StlTm" type="ISODateTime" minOccurs="0"/>
				<xs:element ref="StlTp"/>
				<xs:element ref="SetMode"/>
				<xs:element ref="ClrTp"/>
				<xs:element name="SetAmt" type="CurrencyAndAmount" minOccurs="0"/>
				<xs:element name="PayInt" type="CurrencyAndAmount" minOccurs="0"/>
				<xs:element name="DrftNm" type="Max10NumericText"/>
				<xs:element name="DealId" type="Max20AlphaNumericText" minOccurs="0"/>
				<xs:element name="CdNo" type="Max30NumericText"/>
				<xs:element name="CdRange" type="Max25Text" minOccurs="0"/>
				<xs:element ref="RcvInf" minOccurs="0"/>
				<xs:element ref="RcvCorpInf" minOccurs="0"/>
				<xs:element ref="PayInf" minOccurs="0"/>
				<xs:element ref="PayCorpInf" minOccurs="0"/>
				<xs:element name="CcpcMsgId" type="Max35NumericText" minOccurs="0"/>				
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="StlRstInf">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="StlRlt"/>
				<xs:element ref="StlFailRsn" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExtInfs">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ExtInf" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExtInf">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Name" type="Max150Text"/>
				<xs:element name="Value" type="Max500Text" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RcvInf">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="BrId" type="MaxMin9NumericText"/>
				<xs:element name="TAcctNo" type="Max32AlphaNumericText" />
				<xs:element name="TAcctNm" type="Max150Text" />
				<xs:element name="FAcctNo" type="Max32AlphaNumericText" minOccurs="0"/>
				<xs:element name="FAcctNm" type="Max150Text" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PayInf">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="BrId" type="MaxMin9NumericText"/>
				<xs:element name="TAcctNo" type="Max32AlphaNumericText" />
				<xs:element name="TAcctNm" type="Max150Text" />
				<xs:element name="FAcctNo" type="Max32AlphaNumericText" minOccurs="0"/>
				<xs:element name="FAcctNm" type="Max150Text" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RcvCorpInf">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="OpenBrId" type="MaxMin9NumericText" minOccurs="0"/>
				<xs:element name="OpBkId" type="MaxMin12NumericText" minOccurs="0"/>
				<xs:element name="SocCode" type="MaxMin18AlphaNumericSymbolText" minOccurs="0"/>
				<xs:element name="Acct" type="Max32AlphaNumericText" minOccurs="0"/>
				<xs:element name="Name" type="Max150Text" minOccurs="0"/>
				<xs:element name="CdAcctNo" type="Max32AlphaNumericSymbolText" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PayCorpInf">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="OpenBrId" type="MaxMin9NumericText"  minOccurs="0"/>
				<xs:element name="OpBkId" type="MaxMin12NumericText" minOccurs="0"/>
				<xs:element name="SocCode" type="MaxMin18AlphaNumericSymbolText" minOccurs="0"/>
				<xs:element name="Acct" type="Max32AlphaNumericText" minOccurs="0"/>
				<xs:element name="Name" type="Max150Text" minOccurs="0"/>
				<xs:element name="CdAcctNo" type="Max32AlphaNumericSymbolText" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="StlTp">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="RE1011"/>
				<xs:enumeration value="RE1021"/>
				<xs:enumeration value="RE1022"/>
				<xs:enumeration value="RE1023"/>
				<xs:enumeration value="RE1024"/>
				<xs:enumeration value="RE1031"/>
				<xs:enumeration value="RE1032"/>
				<xs:enumeration value="RE2011"/>
				<xs:enumeration value="RE2021"/>
				<xs:enumeration value="RE3011"/>
				<xs:enumeration value="RE3021"/>
				<xs:enumeration value="RE3022"/>
				<xs:enumeration value="RE3023"/>
				<xs:enumeration value="RE3024"/>
				<xs:enumeration value="RE4011"/>
				<xs:enumeration value="RE6011"/>
				<!-- add kfzx-wang 2021-10-26  新增回购式贴现,回购式贴现赎回,批量清算类型-->
				<xs:enumeration value="RE4021"/>
				<xs:enumeration value="RE4022"/>
				<xs:enumeration value="RE7011"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="SetMode">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="ST01"/>
				<xs:enumeration value="ST02"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="ClrTp">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="CT01"/>
				<xs:enumeration value="CT02"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="StlRlt">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="R20"/>
				<xs:enumeration value="R21"/>
				<xs:enumeration value="R23"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="StlFailRsn">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="SF00"/>
				<xs:enumeration value="SF01"/>
				<xs:enumeration value="SF02"/>
				<xs:enumeration value="SF03"/>
				<xs:enumeration value="SF04"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:simpleType name="Signature">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="2000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Max35NumericText">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="35"/>
			<xs:pattern value="[0-9]{1,35}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ISODateTime">
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="Max24AlphaNumericText">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="24"/>
			<xs:pattern value="[a-zA-Z0-9]{1,24}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CurrencyAndAmount_SimpleType">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="(\d{1,16})|(\d{1,16}\.\d{1,2})"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="CurrencyAndAmount">
		<xs:simpleContent>
			<xs:extension base="CurrencyAndAmount_SimpleType">
				<xs:attribute name="Ccy" type="CurrencyCode" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="CurrencyCode">
		<xs:restriction base="xs:string">
			<xs:pattern value="CNY"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Max10NumericText">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="10"/>
			<xs:pattern value="[0-9]{1,10}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Max20AlphaNumericText">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="20"/>
			<xs:pattern value="[a-zA-Z0-9]{1,20}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Max30NumericText">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="30"/>
			<xs:pattern value="[0-9]{1,30}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MaxMin9NumericText">
		<xs:restriction base="xs:string">
			<xs:minLength value="9"/>
			<xs:maxLength value="9"/>
			<xs:pattern value="[0-9]{1,9}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MaxMin12NumericText">
		<xs:restriction base="xs:string">
			<xs:minLength value="12"/>
			<xs:maxLength value="12"/>
			<xs:pattern value="[0-9]{1,12}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Max32AlphaNumericText">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="32"/>
			<xs:pattern value="[a-zA-Z0-9]{1,32}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MaxMin18AlphaNumericSymbolText">
		<xs:restriction base="xs:string">
			<xs:minLength value="18"/>
			<xs:maxLength value="18"/>
			<xs:pattern value="[a-zA-Z0-9]{1,18}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Max32AlphaNumericSymbolText">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="32"/>
			<xs:pattern value="[a-zA-Z0-9]{1,32}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Max150Text">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="150"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Max500Text">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="500"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Max25Text">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="25"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>